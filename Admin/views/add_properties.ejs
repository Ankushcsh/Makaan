<%- include("common/head") %>


    <%-include("common/navbar") %>


        <div class="panel panel-info">
            <div class="panel-heading">
                ADD PROPERTY
            </div>
            <div class="panel-body">
                <form role="form" action="/add_property" method="post" enctype="multipart/form-data">
                    <div class="form-group">
                        <label>Property Name</label>
                        <input class="form-control" type="text" name="Pname">
                        <p class="help-block">Help text here.</p>
                    </div>
                    <div class="form-group">
                        <label>ADDRESS</label>
                        <input class="form-control" type="text" name="ADDRESS">
                        <p class="help-block">Help text here.</p>
                    </div>
                    <div class="form-group">
                        <label>SIZE in sqft</label>
                        <input class="form-control" type="text" name="size">
                        <p class="help-block">Help text here.</p>
                    </div>
                    <div class="form-group">
                        <label>Bedroom</label>
                        <input class="form-control" type="number" name="bed">
                        <p class="help-block">Help text here.</p>
                    </div>
                    <div class="form-group">
                        <label>Washroom</label>
                        <input class="form-control" type="number" name="wroom">
                        <p class="help-block">Help text here.</p>
                    </div>
                    <div class="form-group">
                        <label>Property Agent</label>
                        <select class="form-control" name="agent">
                            <option value="">select Agent Name</option>
                            <% for(let k of sucess.adata) {%>
                                <option value="<%=k._id%>">
                                    <%=k.name%>
                                </option>
                                <% } %>

                        </select>
                    </div>


                    <div class="form-group">
                        <label>Property Catagory</label>
                        <select class="form-control" name="catagory">
                            <option value="">Select Catagory</option>
                            <% for(let k of sucess.pcat) {%>
                                <option value="<%=k._id%>">
                                    <%=k.cat%>
                                </option>
                                <% } %>

                        </select>
                    </div>
                    <div class="form-group">
                        <label>Property Type</label>
                        <select class="form-control" name="types">
                            <option value="">Select Type</option>
                            <% for(let k of sucess.ptyp) {%>
                                <option value="<%=k._id%>">
                                    <%=k.types%>
                                </option>
                                <% } %>

                        </select>
                    </div>

                    <div class="form-group">
                        <label>Price</label>
                        <input class="form-control" type="text" name="Price">
                        <p class="help-block">Enter Value.</p>
                    </div>
                    <div class="form-group">
                        <label>Description</label>
                        <textarea class="form-control" rows="3" type="text" name="Pdesc"></textarea>
                    </div>
                    <div class="form-group">
                        <label>Property Image</label>
                        <input class="form-control" type="file" name="image">
                        <p class="help-block">image.</p>


                        <button type="submit" class="btn btn-info">SUBMIT </button>
                        <a href="/showprop"> show all</a>


                </form>
            </div>
        </div>
        </div>



        <%- include("common/footer") %>